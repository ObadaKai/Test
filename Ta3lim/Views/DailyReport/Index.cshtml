@model IEnumerable<Ta3lim.Database.DailyActivity>

@{
    ViewBag.Title = "Index";
}

<h2>لائحة النشاطات اليومية</h2>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Employee.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee.surname)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Observation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Report)
        </th>



    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Employee.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.surname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Report)
        </td>



    </tr>
}

</table>
